games = [{home_team: "Pats", away_team: "Broncos", home_score: 7, away_score: 3 },]

away_team_wins = 0
away_team_losses = 0
home_team_wins = 0
home_team_losses = 0

if games[0][:home_score] > games[0][:away_score]
  home_team_wins += 1 && away_team_losses += 1
else
  away_team_wins += 1 && home_team_losses += 1
end
